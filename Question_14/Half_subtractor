def Xor_Gate(a,b):
    if (a!=b):
        return 1
    return 0

def And_Gate(a,b):
    if (Not_Gate(a)==1 and b==1):
        return 1
    return 0

def Not_Gate(a):
    if a==0:
        return 1
    return 0

a=int(input('a:'))
b=int(input('b:'))
diff=Xor_Gate(a,b)
borrow=And_Gate(a,b)
print('difference',diff)
print('borrow',borrow)
